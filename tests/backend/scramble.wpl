let comp scrambler() =
  var scrmbl_st: arr[7] bit := {'1,'1,'1,'1,'1,'1,'1}; 
  var tmp: bit;
  var y:bit;
  
  repeat [8,8]
    seq {
      x <- take;
    
      do {
        tmp := (scrmbl_st[3] ^ scrmbl_st[0]);
        scrmbl_st[0:5] := scrmbl_st[1:6];
        scrmbl_st[6] := tmp;
        y := x ^ tmp;
        print y;
      }; 

      emit (y)
    }
in


read >>> scrambler() >>> write



