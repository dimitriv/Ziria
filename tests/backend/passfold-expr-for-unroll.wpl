fun f(a : int) = 
	var x : int := 0 in
	begin 
  if a == 0 then 
    unroll for i in [0, 0] do x := x + 2 done
  else if a == 1 then
    for i in [0, 1] do x := x + 2 done 
  else if a == 2 then
    for i in [0, 2] do x := x + 2 done
  else if a == 3 then
    for i in [0, 3] do x := x + 2 done
  else if a == 4 then
    for i in [0, 4] do x := x + 2 done
  else if a == 5 then 
    for i in [0, 5] do x := x + 2 done
  else if a == 6 then
    for i in [0, 6] do x := x + 2 done
  else if a == 7 then
    for i in [0, 7] do x := x + 2 done
  else if a == 8 then
    for i in [0, 8] do x := x + 2 done
  else if a == 9 then
    for i in [0, 9] do x := x + 2 done
  else
    for i in [0, a] do x := x + 2 done;
  x
	end
 




let comp main = read[int] >>> map(f) >>> write[int]
